#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "snode.h"
struct snode *snode_create(char* s, int lengthS){
            struct snode *new_snode;
            new_snode = malloc(sizeof(struct snode));
            strcpy(new_snode->str,s);
            new_snode -> length = lengthS;
            new_snode -> next = NULL;
            return new_snode;
}

